<html>
    <head>
        <title>Todo</title>
        <link rel="stylesheet" href="/assets/main.css" type="text/css">
    </head>
    <body>
        <form action="">
            <input type="text" name="item" placeholder="Task Here..">
            <button type="submit">ADD</button>
        </form>
        <ul>
            <% todo_data.forEach(function (x) { %>
                <li><%= x.item %></li>
            <% }); %>
        </ul>
    </body>
</html>
